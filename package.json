{
  "name": "agency-zero-starter-v4",
  "version": "1.0.0",
  "description": "using next.js and apollo",
  "scripts": {
    "analyze:client": "BUNDLE_ANALYZE=client next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:both": "BUNDLE_ANALYZE=both next build",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "dev": "node server/index.js",
    "next": "next",
    "build": "next build",
    "start": "NODE_ENV=production node server/index.js",
    "export": "next build && next export",
    "update": "git pull origin master && npm run build && pm2 restart all",
    "docker:build": "docker build -t <containername> .",
    "docker:clean": "docker rm -f <containername> || true",
    "docker:run": "docker run -p 80:3000 -d --name <containername> <containername> && npm run docker:logs",
    "docker:stop": "docker stop <containername>",
    "docker:start": "docker start <containername> && npm run docker:logs",
    "docker:logs": "docker logs -f <containername>",
    "deploy": "npm run docker:build && npm run docker:clean && npm run docker:run",
    "dc:up": "docker-compose up",
    "dc:down": "docker-compose down"
  },
  "author": "Will Meier",
  "license": "ISC",
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**"
    ]
  },
  "dependencies": {
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-workers": "^1.0.0",
    "apollo-client-preset": "^1.0.8",
    "autoprefixer": "^7.2.6",
    "babel-core": "^7.0.0-bridge.0",
    "compression": "^1.7.3",
    "dotenv": "^5.0.1",
    "dotenv-load": "^1.1.0",
    "express": "^4.16.3",
    "graphql": "^0.11.7",
    "graphql-anywhere": "^4.1.19",
    "graphql-tag": "^2.9.2",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-unfetch": "^2.1.1",
    "loaders.css": "^0.1.2",
    "next": "^7.0.0",
    "next-compose-plugins": "^2.1.1",
    "next-env": "^1.0.1",
    "next-page-transitions": "^1.0.0-alpha.4",
    "next-routes": "^1.4.2",
    "next-url-prettifier": "^1.3.0",
    "node-sass": "^4.9.3",
    "normalize.css": "^7.0.0",
    "postcss-url": "^8.0.0",
    "qs": "^6.5.2",
    "raf": "^3.4.0",
    "react": "^16.5.2",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.5.2",
    "react-loaders": "^3.0.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.3.0",
    "sitemap": "^1.13.0",
    "styled-jsx-plugin-postcss": "^0.1.3",
    "worker-loader": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-jest": "^22.4.4",
    "babel-plugin-inline-react-svg": "^0.5.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "chai": "^4.1.2",
    "chai-jquery": "^2.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "glob": "^7.1.3",
    "jest": "^22.4.4",
    "jest-cli": "^22.4.4",
    "jquery": "^3.3.1",
    "jsdom": "^11.12.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.1.6",
    "prop-types": "^15.6.2",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^16.5.2",
    "sass-loader": "^6.0.7",
    "standard": "^10.0.3"
  }
}
